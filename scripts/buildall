#!/bin/sh

PROJECTS="armemu-test beaglebone-test sam7ex256-test osk5912-test beagle-test stm32-h103-test stm32-p107-test sam3x-ek-test stm3220g-eval"
FAILED=""

for p in $PROJECTS; do
	PROJECT=$p nice make -j4 || FAILED="$FAILED $p"
done

if [ "$FAILED" != "" ]; then
	echo
	echo some projects have failed to build:
	echo $FAILED
fi
