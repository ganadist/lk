LOCAL_DIR := $(GET_LOCAL_DIR)

INCLUDES += -I$(LOCAL_DIR)/include

PLATFORM := msm7k

MEMBASE := 0x00000000 # SMI
MEMSIZE := 0x00800000 # 8MB

LINUX_MACHTYPE := 0x0000059F

KEYS_USE_GPIO_KEYPAD := 1

MODULES += \
	dev/keys \
	lib/ptable

DEFINES += \
	SDRAM_SIZE=$(MEMSIZE) \
	MEMBASE=$(MEMBASE) \
	LINUX_MACHTYPE=$(LINUX_MACHTYPE)

OBJS += \
	$(LOCAL_DIR)/init.o \
	$(LOCAL_DIR)/keypad.o \
	$(LOCAL_DIR)/panel.o
