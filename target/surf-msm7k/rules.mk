LOCAL_DIR := $(GET_LOCAL_DIR)

MODULE := $(LOCAL_DIR)

INCLUDES += -I$(LOCAL_DIR)/include

PLATFORM := msm7k

MEMBASE := 0x00000000 # SMI
MEMSIZE := 0x00800000 # 8MB

LINUX_MACHTYPE := 0x0000059F

KEYS_USE_GPIO_KEYPAD := 1

MODULE_DEPS += \
	dev/keys \
	lib/ptable

DEFINES += \
	SDRAM_SIZE=$(MEMSIZE) \
	MEMBASE=$(MEMBASE) \
	LINUX_MACHTYPE=$(LINUX_MACHTYPE)

MODULE_SRCS += \
	$(LOCAL_DIR)/init.c \
	$(LOCAL_DIR)/keypad.c \
	$(LOCAL_DIR)/panel.c

include make/module.mk
